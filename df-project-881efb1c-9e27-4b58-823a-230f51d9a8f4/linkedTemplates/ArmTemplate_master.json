{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-project-881efb1c-9e27-4b58-823a-230f51d9a8f4"
		},
		"blobStorageLinkedService_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'blobStorageLinkedService'"
		},
		"blobStorageLinkedService_properties_typeProperties_sasToken_secretName": {
			"type": "string",
			"defaultValue": "blob-storage-access-key"
		},
		"datalakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlproject881efb1c9e274b5.dfs.core.windows.net/"
		},
		"keyVaultLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://bigdata-project-kv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-project-881efb1c-9e27-4b58-823a-230f51d9a8f4_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"keyVaultLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('keyVaultLinkedService_properties_typeProperties_baseUrl')]"
					},
					"blobStorageLinkedService_sasUri": {
						"value": "[parameters('blobStorageLinkedService_sasUri')]"
					},
					"blobStorageLinkedService_properties_typeProperties_sasToken_secretName": {
						"value": "[parameters('blobStorageLinkedService_properties_typeProperties_sasToken_secretName')]"
					},
					"datalakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('datalakeLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}